@model DataModel.MemberLog

    <div class="card shadow mb-4">
        <div class="card-header py-3">
            <div class="d-sm-flex align-items-center justify-content-between">
                <h6 class="m-0 font-weight-bold text-primary">Member Log Details</h6>
            </div>
        </div>
        <div class="card-body">

            <div class="row mb-1">
                <div class="col-lg-2 col-md-6 col-sm-12">Subscribe Datetime</div>
                <div class="col-lg-8 col-md-6 col-sm-12">
                    @Html.DisplayFor(m => m.LogDateTime, "{0:dd/MM/yyyy HH:mm:ss}", new { @class = "form-control" })
                </div>
            </div>

            <div class="row mb-1">
                <div class="col-lg-2 col-md-6 col-sm-12">Activity</div>
                <div class="col-lg-8 col-md-6 col-sm-12">
                    @Html.DisplayFor(m => m.Activity, new { @class = "form-control" })
                </div>
            </div>
            <div class="row mb-1">
                <div class="col-lg-2 col-md-6 col-sm-12">Status</div>
                <div class="col-lg-8 col-md-6 col-sm-12">
                    @Html.DisplayFor(m => m.Status, new { @class = "form-control" })
                </div>
            </div>
            <div class="row mb-1">
                <div class="col-lg-2 col-md-6 col-sm-12">Details</div>
                <div class="col-lg-8 col-md-6 col-sm-12">
                    @Model.Details
                </div>
            </div>
            <div class="row mb-1">
                <div class="col-lg-2 col-md-6 col-sm-12">Error Massage</div>
                <div class="col-lg-8 col-md-6 col-sm-12">
                    @Html.DisplayFor(m => m.ErrorMsg, new { @class = "form-control" })
                </div>
            </div>

            <div class="card-footer">
                <div class="text-right">

                    <button type="submit" id="btn_submit" style="display: none;"></button>
                    <a href="@Url.Action("MemberLogList")" class=" d-sm-inline-block btn btn-sm btn-warning shadow-sm">
                        <i class="fas fa-chevron-circle-left fa-sm text-white-50"></i>
                        Cancel
                    </a>
                </div>
            </div>
        </div>
    </div>

