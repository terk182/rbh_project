@model  BL.Entities.GogojiiFare.AirFare

@*<form id="form1" onSubmit="return check(this)" name="form1" method="post">*@
@using (Html.BeginForm("FlightReportReissueDetailsave", "ReportFlight", FormMethod.Post, new { data_parsley_validate = "", @class = "form-horizontal form-label-left", role = "form", enctype = "multipart/form-data", @onSubmit = "return check(this)" }))
{
    @Html.HiddenFor(m => m.bookingOID)


    <div class="row">
        <div class="col-lg-12">
            <div class="card-header py-3" style="text-align:right;">
                @*<div class="d-sm-flex align-items-center justify-content-between">*@
                <a href="@Url.Action("FlightReportList")" class=" d-sm-inline-block btn btn-sm btn-warning shadow-sm">
                    <i class="fas fa-chevron-circle-left fa-sm text-white-50"></i>
                    Back
                </a>

                <button type="submit" class="d-sm-inline-block btn btn-sm btn-primaryblue shadow-sm" id="btn_Save">
                    <i class="fas fa-save fa-sm text-white-50"></i>
                    &nbsp;&nbsp;Save &nbsp;&nbsp;
                </button>
                @*</div>*@
            </div>
        </div>
    </div>


    <div class="row">
        <div class="col-lg-6">
            <div class="card shadow mb-4">
                <div class="card-header py-3">
                    <div class="d-sm-flex align-items-center justify-content-between">
                        <h6 class="m-0 font-weight-bold text-primary"><i class="fas fa-edit"></i>&nbsp;Ticketing Task</h6>
                    </div>
                </div>
                <div class="card-body">
                    <div class="row mb-1">
                        <div class="col-lg-3 col-md-5 col-sm-5" style="text-align:right;">
                            Status :
                        </div>
                        <div class="col-lg-4 col-md-5 col-sm-5 text">
                            @{
                                SelectList selectListStatusReissue = (SelectList)ViewData["StatusReissueList"];
                            }
                            @*//ใน text บวก value จาก controller มาด้วย*@
                            @Html.DropDownListFor(m => m.reissue.status, selectListStatusReissue.OrderBy(x => x.Value).Select(s => new SelectListItem { Text = (s.Text), Value = s.Value.ToString(), Selected = (Model.reissue.status == Convert.ToInt32(s.Value)) }), new
                                {
                                    @class = "form-control ADT-TT-",
                                    required = "required"
                                })

                        </div>



                        <div class="col-lg-2 col-md-5 col-sm-5" style="text-align:right;">
                            New PNR :
                        </div>
                        <div class="col-lg-3 col-md-5 col-sm-5 text">
                            @Html.TextBoxFor(x => x.reissue.newPNR, new { @class = "form-control form-control-sm" })
                        </div>


                        <div class="col-lg-3 col-md-5 col-sm-5" style="text-align:right;">
                            Reissue Create Date :
                        </div>
                        <div class="col-lg-4 col-md-5 col-sm-5 text">
                            @*<input type="text" class="form-control form-control-sm" value="@Model.refund.refundCreateDate.ToString("MM/dd/yyyy")" readonly>*@
                            @Html.TextBoxFor(m => m.reissue.reissueCreateDate, "{0:dd/MM/yyyy}", new { required = "required", @class = "form-control form-control-sm", @id = "ReissueCreateDate", @name = "ReissueCreateDate", @readonly = "readonly" })
                            @Html.Hidden("reissueCreateDate", Model.reissue.reissueCreateDate)
                        </div>

                        <div class="col-lg-2 col-md-5 col-sm-5" style="text-align:right;">
                            Type Chage :
                        </div>
                        <div class="col-lg-3 col-md-5 col-sm-5 text">
                            @{
                                SelectList selectListTypeChage = (SelectList)ViewData["TypeChangeList"];
                            }
                            @Html.DropDownListFor(m => m.reissue.typeChage, selectListTypeChage.OrderBy(x => x.Value).Select(s => new SelectListItem { Text = (s.Text), Value = s.Value.ToString(), Selected = (Model.reissue.typeChage == Convert.ToInt32(s.Value)) }), new
                                {
                                    @class = "form-control ADT-TT-",
                                    required = "required"
                                })
                        </div>

                        <div class="col-lg-3 col-md-5 col-sm-5" style="text-align:right;">
                            Detail Change :
                        </div>
                        <div class="col-lg-5 col-md-5 col-sm-5 text">
                            @{
                                SelectList selectListDetailChange = (SelectList)ViewData["DetailChangeList"];
                            }
                            @Html.DropDownListFor(m => m.reissue.detailChage, selectListDetailChange.OrderBy(x => x.Value).Select(s => new SelectListItem { Text = (s.Text), Value = s.Value.ToString(), Selected = (Model.reissue.detailChage == Convert.ToInt32(s.Value)) }), new
                                {
                                    @class = "form-control ADT-TT-",
                                    required = "required"
                                })
                        </div>

                        <div class="col-lg-3 col-md-4 col-sm-5"></div>

                        <div class="col-lg-3" style="text-align:right;">
                            Remark :
                        </div>
                        <div class="col-lg-9 text">
                            @Html.TextAreaFor(m => m.reissue.remark, new { @class = "form-control form-control-sm", @style = "font-size:18px;" })
                        </div>



                    </div>

                </div>
                <div class="card-footer">
                </div>
            </div>
        </div>


        <div class="col-lg-6">
            <div class="card shadow mb-4">
                <div class="card-header py-3">
                    <div class="d-sm-flex align-items-center justify-content-between">
                        <h6 class="m-0 font-weight-bold text-primary"><i class="fas fa-edit"></i>&nbsp;Booking Detail</h6>
                    </div>
                </div>
                <div class="card-body">
                    <div class="row mb-1">
                        <div class="col-lg-3" style="text-align:right;">
                            PNR :
                        </div>
                        <div class="col-lg-3 text">
                            <span style="background-color:#FBAC02;padding:2px 10px;border-radius:10px;color:white;"><b>@Model.PNR</b></span>
                        </div>



                        <div class="col-lg-3 col-md-5 col-sm-5" style="text-align:right;">
                            Booking Date :
                        </div>
                        <div class="col-lg-3 col-md-5 col-sm-5 text">
                            @Model.bookingDate.ToString("dd MMMM yyyy HH:mm:ss tt")
                            @*@Html.TextBoxFor(m => m.bookingDate, "{0:yyyy-MM-dd}", new { required = "required", @class = "form-control form-control-sm", @type = "date", @id = "datetime1", @readonly = "readonly" })*@
                        </div>



                        <div class="col-lg-3 col-md-5 col-sm-5" style="text-align:right;">
                            From City :
                        </div>
                        <div class="col-lg-3 col-md-5 col-sm-5 text">
                            @if (Model.depFlight != null && Model.depFlight.Count > 0)
                            {
                                @Model.depFlight[0].depCity.code
                            }
                        </div>

                        <div class="col-lg-3 col-md-5 col-sm-5" style="text-align:right;">
                            To City :
                        </div>
                        <div class="col-lg-3 col-md-5 col-sm-5 text">
                            @if (Model.depFlight != null && Model.depFlight.Count > 0)
                            {
                                @Model.depFlight[Model.depFlight.Count - 1].arrCity.code
                            }
                        </div>

                        <div class="col-lg-3 col-md-5 col-sm-5" style="text-align:right;">
                            Airline :
                        </div>
                        <div class="col-lg-3 col-md-5 col-sm-5 text">
                            @if (Model.depFlight != null && Model.depFlight.Count > 0)
                            {
                                @Model.depFlight[0].airline.code
                            }
                        </div>

                        <div class="col-lg-3 col-md-5 col-sm-5" style="text-align:right;">
                            No Of Pax :
                        </div>
                        <div class="col-lg-3 col-md-5 col-sm-5 text">
                            @(Model.noOfAdults + Model.noOfChildren + Model.noOfInfants)
                            @*<input type="text" class="form-control form-control-sm" value="@(Model.noOfAdults+Model.noOfChildren+Model.noOfInfants)" readonly>*@
                        </div>

                        <div class="col-lg-3 col-md-5 col-sm-5" style="text-align:right;">
                            Segment/PNR :
                        </div>
                        <div class="col-lg-3 col-md-5 col-sm-5 text">
                            -
                        </div>

                        <div class="col-lg-3 col-md-5 col-sm-5" style="text-align:right;">
                            All Segment :
                        </div>
                        <div class="col-lg-3 col-md-5 col-sm-5 text">
                            -
                        </div>

                        <div class="col-lg-3 col-md-5 col-sm-5" style="text-align:right;">
                            Discount :
                        </div>
                        <div class="col-lg-3 col-md-5 col-sm-5 text">
                            -
                        </div>

                        <div class="col-lg-3 col-md-5 col-sm-5" style="text-align:right;">
                            Total(฿) :
                        </div>
                        <div class="col-lg-3 col-md-5 col-sm-5 text">
                            @Model.grandTotal.ToString("N0")
                        </div>
                    </div>

                </div>
                <div class="card-footer">
                    <div class="text-right">

                    </div>
                </div>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="col-lg-12">
            <div class="card shadow mb-4">
                <div class="card-header py-3">
                    <div class="d-sm-flex align-items-center justify-content-between">
                        <h6 class="m-0 font-weight-bold text-primary"><i class="fas fa-edit"></i>&nbsp;Contact Detail</h6>
                    </div>
                </div>
                <div class="card-body">
                    <div class="row mb-1">
                        @if (Model.contactInfo.firstname != null && Model.contactInfo.email != null && Model.contactInfo.telNo != null)
                        {
                            <div class="col-lg-1">
                                Title
                            </div>
                            <div class="col-lg-2 text">
                                <input type="text" class="form-control form-control-sm" value="@Model.contactInfo.title" readonly>
                            </div>

                            <div class="col-lg-1 col-md-5 col-sm-5">
                                First Name
                            </div>
                            <div class="col-lg-2 col-md-5 col-sm-5 text">
                                <input type="text" class="form-control form-control-sm" value="@Model.contactInfo.firstname" readonly>
                            </div>

                            <div class="col-lg-1 col-md-5 col-sm-5">
                                Middle Name
                            </div>
                            <div class="col-lg-2 col-md-5 col-sm-5 text">
                                <input type="text" class="form-control form-control-sm" value="@Model.contactInfo.middlename" readonly>
                            </div>

                            <div class="col-lg-1 col-md-5 col-sm-5">
                                Last Name
                            </div>
                            <div class="col-lg-2 col-md-5 col-sm-5 text">
                                <input type="text" class="form-control form-control-sm" value="@Model.contactInfo.lastname" readonly>
                            </div>

                            <div class="col-lg-1 col-md-5 col-sm-5">
                                Email
                            </div>
                            <div class="col-lg-2 col-md-5 col-sm-5 text">
                                <input type="text" class="form-control form-control-sm" value="@Model.contactInfo.email" readonly>
                            </div>

                            <div class="col-lg-1 col-md-5 col-sm-5">
                                Phone number
                            </div>
                            <div class="col-lg-2 col-md-5 col-sm-5 text">
                                <input type="text" class="form-control form-control-sm" value="@Model.contactInfo.telNo" readonly>
                            </div>
                        }
                    </div>

                </div>
                <div class="card-footer">
                    <div class="text-right">
                    </div>
                </div>
            </div>
        </div>
    </div>


    <div class="card shadow mb-4">
        <div class="card-header py-3">
            <div class="d-sm-flex align-items-center justify-content-between">
                <h6 class="m-0 font-weight-bold text-primary"><i class="fas fa-plane"></i>&nbsp;Passenger Details</h6>

            </div>
        </div>
        <div class="card-body">


            <div class="row" id="checkRefund" name="checkRefund">
                <table class="table table-bordered">

                    <thead class="table">
                        <tr>
                            <th><input type="checkbox" name="select-all" id="select-all" /></th>
                            <th>PaxNo</th>
                            <th>PaxType</th>
                            <th>Title</th>
                            <th>First Name</th>
                            <th>Middle Name</th>
                            <th>Last Name</th>
                            <th>Net (Reissue/Revalidate)</th>
                            <th>Agent (Reissue/Revalidate)</th>
                            <th>Selling (Reissue/Revalidate)</th>
                        </tr>
                    </thead>
                    <tbody>
                        @{
                            int paxNo = 1;
                        }
                        @for (int i = 0; i < Model.adtPaxs.Count; i++)
                        {
                            if (Model.adtPaxs[i].StatusReissue == true)
                            {
                                <tr>
                                    <td></td>
                                    <td>@(paxNo.ToString())</td>
                                    <td>@Model.adtPaxs[i].paxType</td>
                                    <td>@Model.adtPaxs[i].title</td>
                                    <td>@Model.adtPaxs[i].firstname</td>
                                    <td>@Model.adtPaxs[i].middlename</td>
                                    <td>@Model.adtPaxs[i].lastname</td>
                                    <td>@Html.TextBoxFor(x => x.adtPaxs[i].netReissue, new { @class = "form-control form-control-sm", @placeholder = "Net (Refund)", @readonly = "readonly" })</td>
                                    <td>@Html.TextBoxFor(x => x.adtPaxs[i].agentReissue, new { @class = "form-control form-control-sm", @placeholder = "Agent (Refund)", @readonly = "readonly" })</td>
                                    <td>@Html.TextBoxFor(x => x.adtPaxs[i].reissueSelling, new { @class = "form-control form-control-sm", @placeholder = "Refund Amount", @readonly = "readonly" })</td>
                                    @*<td>@Model.adtFare.sellingBaseFare.ToString("N2")</td>*@
                                </tr>
                                <tr>
                                    <td colspan="2" style="text-align:right;">TicketNo. (Old):</td>
                                    <td colspan="2">@Html.TextBoxFor(x => x.adtPaxs[i].tickNoReissueOld, new { @class = "form-control form-control-sm", @placeholder = "TicketNo.", @readonly = "readonly" })</td>
                                    <td colspan="2" style="text-align:right;">TicketNo. (New):</td>
                                    <td colspan="2">@Html.TextBoxFor(x => x.adtPaxs[i].tickNoReissueNew, new { @class = "form-control form-control-sm", @placeholder = "TicketNo.", @readonly = "readonly" })</td>
                                    <td colspan="4">
                                        @if (Model.adtPaxs[i].StatusReissue == true)
                                        {
                                            <span style="background-color:#FBAC02;padding:2px 10px;border-radius:10px;color:white;">
                                                <b>
                                                    <label>Reissue</label>
                                                </b>
                                            </span>

                                        }
                                    </td>
                                </tr>
                                <tr>
                                    <td colspan="2" style="text-align:right;">Remark:</td>
                                    <td colspan="10">@Html.TextBoxFor(x => x.adtPaxs[i].remarkReissue, new { @class = "form-control form-control-sm", @placeholder = "Remark", @readonly = "readonly" })</td>
                                </tr>


                                paxNo++;
                            }
                            else
                            {
                                <tr>
                                    <td>@Html.CheckBoxFor(m => m.adtPaxs[i].StatusReissue, new { @type = "checkbox" })</td>
                                    <td>@(paxNo.ToString())</td>
                                    <td>@Model.adtPaxs[i].paxType</td>
                                    <td>@Model.adtPaxs[i].title</td>
                                    <td>@Model.adtPaxs[i].firstname</td>
                                    <td>@Model.adtPaxs[i].middlename</td>
                                    <td>@Model.adtPaxs[i].lastname</td>
                                    <td>@Html.TextBoxFor(x => x.adtPaxs[i].netReissue, new { @class = "form-control form-control-sm", @placeholder = "Net (Refund)", @disabled = "disabled" })</td>
                                    <td>@Html.TextBoxFor(x => x.adtPaxs[i].agentReissue, new { @class = "form-control form-control-sm", @placeholder = "Agent (Refund)", @disabled = "disabled" })</td>
                                    <td>@Html.TextBoxFor(x => x.adtPaxs[i].reissueSelling, new { @class = "form-control form-control-sm", @placeholder = "Refund Amount", @disabled = "disabled" })</td>
                                    @*<td>@Model.adtFare.sellingBaseFare.ToString("N2")</td>*@
                                </tr>
                                <tr>
                                    <td colspan="2" style="text-align:right;">TicketNo. (Old):</td>
                                    <td colspan="2">@Html.TextBoxFor(x => x.adtPaxs[i].tickNoReissueOld, new { @class = "form-control form-control-sm", @placeholder = "TicketNo.", @disabled = "disabled" })</td>
                                    <td colspan="2" style="text-align:right;">TicketNo. (New):</td>
                                    <td colspan="2">@Html.TextBoxFor(x => x.adtPaxs[i].tickNoReissueNew, new { @class = "form-control form-control-sm", @placeholder = "TicketNo.", @disabled = "disabled" })</td>
                                    <td colspan="4">
                                        @if (Model.adtPaxs[i].StatusReissue == true)
                                        {
                                            <span style="background-color:#FBAC02;padding:2px 10px;border-radius:10px;color:white;">
                                                <b>
                                                    <label>Reissue</label>
                                                </b>
                                            </span>

                                        }
                                    </td>
                                </tr>
                                <tr>
                                    <td colspan="2" style="text-align:right;">Remark:</td>
                                    <td colspan="10">@Html.TextBoxFor(x => x.adtPaxs[i].remarkReissue, new { @class = "form-control form-control-sm", @placeholder = "Remark", @disabled = "disabled" })</td>
                                </tr>


                                paxNo++;
                            }
                        }

                        @if (Model.chdPaxs.Count > 0 && Model.chdPaxs != null)
                        {
                            for (int j = 0; j < Model.chdPaxs.Count; j++)
                            {
                                if (Model.chdPaxs[j].StatusReissue == true)
                                {
                                    <tr>
                                        <td></td>
                                        <td>@(paxNo.ToString())</td>
                                        <td>@Model.chdPaxs[j].paxType</td>
                                        <td>@Model.chdPaxs[j].title</td>
                                        <td>@Model.chdPaxs[j].firstname</td>
                                        <td>@Model.chdPaxs[j].middlename</td>
                                        <td>@Model.chdPaxs[j].lastname</td>
                                        <td>@Html.TextBoxFor(x => x.chdPaxs[j].netReissue, new { required = "required", @class = "form-control form-control-sm", @placeholder = "Net (Refund)", @readonly = "readonly" })</td>
                                        <td>@Html.TextBoxFor(x => x.chdPaxs[j].agentReissue, new { required = "required", @class = "form-control form-control-sm", @placeholder = "Agent (Refund)", @readonly = "readonly" })</td>
                                        <td>@Html.TextBoxFor(x => x.chdPaxs[j].reissueSelling, new { required = "required", @class = "form-control form-control-sm", @placeholder = "Refund Amount", @readonly = "readonly" })</td>
                                    </tr>
                                    <tr>
                                        <td colspan="2" style="text-align:right;">TicketNo. (Old):</td>
                                        <td colspan="2">@Html.TextBoxFor(x => x.chdPaxs[j].tickNoReissueOld, new { @class = "form-control form-control-sm", @placeholder = "TicketNo.", @readonly = "readonly" })</td>
                                        <td colspan="2" style="text-align:right;">TicketNo. (New):</td>
                                        <td colspan="2">@Html.TextBoxFor(x => x.chdPaxs[j].tickNoReissueNew, new { @class = "form-control form-control-sm", @placeholder = "TicketNo.", @readonly = "readonly" })</td>
                                        <td colspan="4">
                                            @if (Model.chdPaxs[j].StatusReissue == true)
                                            {
                                                <span style="background-color:#FBAC02;padding:2px 10px;border-radius:10px;color:white;">
                                                    <b>
                                                        <label>Reissue</label>
                                                    </b>
                                                </span>

                                            }
                                        </td>
                                    </tr>
                                    <tr>
                                        <td colspan="2" style="text-align:right;">Remark:</td>
                                        <td colspan="10">@Html.TextBoxFor(x => x.chdPaxs[j].remarkReissue, new { @class = "form-control form-control-sm", @placeholder = "Remark", @readonly = "readonly" })</td>
                                    </tr>
                                    paxNo++;
                                }
                                else
                                {
                                    <tr>
                                        <td>@Html.CheckBoxFor(m => m.chdPaxs[j].StatusReissue, new { @type = "checkbox" })</td>
                                        <td>@(paxNo.ToString())</td>
                                        <td>@Model.chdPaxs[j].paxType</td>
                                        <td>@Model.chdPaxs[j].title</td>
                                        <td>@Model.chdPaxs[j].firstname</td>
                                        <td>@Model.chdPaxs[j].middlename</td>
                                        <td>@Model.chdPaxs[j].lastname</td>
                                        <td>@Html.TextBoxFor(x => x.chdPaxs[j].netReissue, new { required = "required", @class = "form-control form-control-sm", @placeholder = "Net (Refund)", @disabled = "disabled" })</td>
                                        <td>@Html.TextBoxFor(x => x.chdPaxs[j].agentReissue, new { required = "required", @class = "form-control form-control-sm", @placeholder = "Agent (Refund)", @disabled = "disabled" })</td>
                                        <td>@Html.TextBoxFor(x => x.chdPaxs[j].reissueSelling, new { required = "required", @class = "form-control form-control-sm", @placeholder = "Refund Amount", @disabled = "disabled" })</td>
                                    </tr>
                                    <tr>
                                        <td colspan="2" style="text-align:right;">TicketNo. (Old):</td>
                                        <td colspan="2">@Html.TextBoxFor(x => x.chdPaxs[j].tickNoReissueOld, new { @class = "form-control form-control-sm", @placeholder = "TicketNo.", @disabled = "disabled" })</td>
                                        <td colspan="2" style="text-align:right;">TicketNo. (New):</td>
                                        <td colspan="2">@Html.TextBoxFor(x => x.chdPaxs[j].tickNoReissueNew, new { @class = "form-control form-control-sm", @placeholder = "TicketNo.", @disabled = "disabled" })</td>
                                        <td colspan="4">
                                            @if (Model.chdPaxs[j].StatusReissue == true)
                                            {
                                                <span style="background-color:#FBAC02;padding:2px 10px;border-radius:10px;color:white;">
                                                    <b>
                                                        <label>Reissue</label>
                                                    </b>
                                                </span>

                                            }
                                        </td>
                                    </tr>
                                    <tr>
                                        <td colspan="2" style="text-align:right;">Remark:</td>
                                        <td colspan="10">@Html.TextBoxFor(x => x.chdPaxs[j].remarkReissue, new { @class = "form-control form-control-sm", @placeholder = "Remark", @disabled = "disabled" })</td>
                                    </tr>
                                    paxNo++;
                                }
                            }

                        }

                        @if (Model.infPaxs.Count > 0 && Model.infPaxs != null)
                        {
                            for (int j = 0; j < Model.infPaxs.Count; j++)
                            {
                                if (Model.infPaxs[j].StatusReissue == true)
                                {
                                    <tr>
                                        <td></td>
                                        <td>@(paxNo.ToString())</td>
                                        <td>@Model.infPaxs[j].paxType</td>
                                        <td>@Model.infPaxs[j].title</td>
                                        <td>@Model.infPaxs[j].firstname</td>
                                        <td>@Model.infPaxs[j].middlename</td>
                                        <td>@Model.infPaxs[j].lastname</td>
