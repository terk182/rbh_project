        .customer-review {
            margin-bottom: 50px;
        }

        .item .customer-container {
            display: -ms-flexbox;
            display: flex;
            -ms-flex-align: center;
            align-items: center;
        }

            .item .customer-container .customer-image {
                min-width: 110px;
                max-width: 110px;
                display: inline-block;
                position: relative;
                padding: 5px;
                text-align: center;
            }

                .item .customer-container .customer-image img {
                    border-radius: 50px;
                }

            .item .customer-container .customer-content {
                margin-left: 92px;
                min-height: 100px;
            }

                .item .customer-container .customer-content .customer-text {
                    position: relative;
                    display: inline;
                    font: inherit;
                    font-size: 14px;
                }

                    .item .customer-container .customer-content .customer-text:before {
                        content: '“';
                        font-family: '';
                        left: -80px;
                        top: 10px;
                        font-size: 10em;
                        line-height: 100px;
                        color: #C4C4C4;
                        width: 20px;
                        height: 20px;
                        position: absolute;
                    }

        .more-than-just-hotel-inter {
            background: #F4F4F4;
            border-radius: 10px;
            padding: 15px;
            width: 100%;
        }

            .more-than-just-hotel-inter .hotel-card .hotel-card__image img {
                min-height: 216px;
                max-height: 216px;
                -o-object-fit: cover;
                object-fit: cover;
                border-radius: 10px;
                width: 100%;
            }

        .hotel-card__badge-container .more-than-just-hotel-card__badge {
            display: inline-block;
            position: relative;
            height: auto;
            background-color: var(--primary-color);
            border-radius: 6px;
            height: 34px;
            min-width: 42px;
            padding: 5px;
            text-align: center;
        }

            .hotel-card__badge-container .more-than-just-hotel-card__badge::before {
                content: " ";
                position: absolute;
                width: 0;
                height: 0;
                left: auto;
                right: -40px;
                top: -8px;
                bottom: auto;
            }

            .hotel-card__badge-container .more-than-just-hotel-card__badge::after {
                content: " ";
                position: absolute;
                width: 0;
                height: 0;
                left: auto;
                right: -10px;
                top: 0px;
                bottom: auto;
                border: 10px solid;
                border-color: var(--primary-color) transparent transparent transparent;
            }

            .hotel-card__badge-container .more-than-just-hotel-card__badge span {
                color: #ffffff;
                font-size: 16px;
                font-weight: bold;
                line-height: 5px;
            }

        .hotel-card__address-and-star {
            display: -ms-flexbox;
            display: flex;
            -ms-flex-align: center;
            align-items: center;
        }

        .star--voted {
            max-width: 14px;
            color: var(--secondary-color) !important;
        }

        .fa-star {
            max-width: 14px;
            color: var(--text-color-light);
        }

        .more-than-just-hotel-inter .body-text, .body-text--primary {
            font-size: 14px;
            line-height: 22px;
            font-weight: 500;
            color: #888888;
        }

        .sub-scribe-deal {
            background: #F4F4F4;
            border-radius: 10px;
            padding: 15px;
            width: 100%;
            text-align: center;
        }

            .sub-scribe-deal .form-email {
