    <script>
        var setLang = function (l) {
            $.post('@Url.Action("ChangeLanguage","Home")?lang=' + l, function () {
                window.location.href = window.location.href.replace('lang=', 'l=');
                window.location.reload();
            });
        };
        $(document).ready(function () {

            $('.owl-carousel').owlCarousel({
                loop: true,
                margin: 10,
                responsiveClass: true,
                responsive: {
                    0: {
                        items: 1,
                        nav: false
                    },
                    600: {
                        items: 1,
                        nav: false
                    },
                    1000: {
                        items: 2,
                        nav: false,
                        loop: false,
                        margin: 20
                    }
                }
            });

        });
        $(".promotionMore").slice(0, 2).show();    
        $("#btnpromotionMore").on('click', function (e) {
            e.preventDefault();
            $(".promotionMore:hidden").slice(0, 1).slideDown();
            if ($(".promotionMore:hidden").length == 0) {
                $("#btnpromotionMore").fadeOut('slow');
            }
        });
        $(".topDomMore").slice(0, 2).show();    
        $("#btntopDomMore").on('click', function (e) {
            e.preventDefault();
            $(".topDomMore:hidden").slice(0, 2).slideDown();
            if ($(".topDomMore:hidden").length == 0) {
                $("#btntopDomMore").fadeOut('slow');
            }
        });
        $(".topInterMore").slice(0, 1).show();    
        $("#btntopInterMore").on('click', function (e) {
            e.preventDefault();
            $(".topInterMore:hidden").slice(0, 2).slideDown();
            if ($(".topInterMore:hidden").length == 0) {
                $("#btntopInterMore").fadeOut('slow');
            }
        });
        $(".moreHotelDomMore").slice(0, 5).show();    
        $("#btnmoreHotelDomMore").on('click', function (e) {
            e.preventDefault();
            $(".moreHotelDomMore:hidden").slice(0, 1).slideDown();
            if ($(".moreHotelDomMore:hidden").length == 0) {
                $("#btnmoreHotelDomMore").fadeOut('slow');
            }
        });
        $(".moreHotelInterMore").slice(0, 5).show();    
        $("#btnmoreHotelInterMore").on('click', function (e) {
            e.preventDefault();
            $(".moreHotelInterMore:hidden").slice(0, 1).slideDown();
            if ($(".moreHotelInterMore:hidden").length == 0) {
                $("#btnmoreHotelInterMore").fadeOut('slow');
            }
        });
        $("#sub-scribe-form").validate({
           rules: {
                field: {
                    required: true,
                    email: true
                }
           },
           messages: {
             user_email: "Please check your email"

           }
        })
        $('#errmsg_Register').hide();
        $("#btnSubscribe").on('click', function () {
            var mailformat =/^([\w-\.]+)@@((\[[0-9]{1,3}\.[0-9]{1,3}\.[0-9]{1,3}\.)|(([\w-]+\.)+))([a-zA-Z]{2,4}|[0-9]{1,3})(\]?)$/;
            $("#sub-scribe-form").valid();
            if ($('#user_email').val() != "" && (mailformat.test($('#user_email').val()) == true)) {
                $(this).removeClass("btn--primary");
                $(this).addClass("text--gray");
                $(this).prop('disabled', true);
                $.ajax({
                    url: "@Url.Action("SubScribeDeals", "Home")" + "?user_email=" + $('#user_email').val(),
                    data: "",
                    cache: true,
                    method: "post",
                    success: function (data) {
                        console.log(data);
                        if (data && data == "OK") {
                            $('#user_email').val('');
                            $('#errmsg_Register').removeClass("text-error");
                            $('#errmsg_Register').addClass("text-sucsess");
                            $('#errmsg_Register').html("@Localize.Show("SUBSCRIBE_COMPLETE")");
                            $('#errmsg_Register').show();
                            $("#btnSubscribe").removeClass("text--gray");
                            $("#btnSubscribe").addClass("btn--primary");
                            $("#btnSubscribe").prop('disabled', false);
                        } else {
                             if ($('#errmsg_Register').find(".text-sucsess")) {
                                    $('#errmsg_Register').removeClass("text-sucsess");
                                    $('#errmsg_Register').addClass("text-error");
                                }
                            $('#errmsg_Register').html("@Localize.Show("SUBSCRIBE_UNCOMPLETE")");
                            $('#errmsg_Register').show();
                            $("#btnSubscribe").removeClass("text--gray");
                            $("#btnSubscribe").addClass("btn--primary");
                            $("#btnSubscribe").prop('disabled', false);
                        }
                    },
                    error: function () {

                    }
                });
            }
        
        });
    </script>
