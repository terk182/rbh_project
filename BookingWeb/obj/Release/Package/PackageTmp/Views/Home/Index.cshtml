            }

        .btn-sub-scribe {
            width: 100% !important;
        }

        .sub-scribe-deal .form-email .body-text {
            color: var(--text-color-light);
        }

        .sub-scribe-deal .form-default .input-group input, .form-default .input-group select {
            height: 50px;
            width: 100%;
            border: 1px solid #dcdcdc;
            border-radius: 10px;
            margin-bottom: 15px;
            padding-left: 35px;
            background: #fff;
        }

        #user_email-error {
            color: red;
        }

        .img-user {
            width: 50px;
            height: 50px;
        }

        .img-member-profile {
            width: 70px;
            border-radius: 50%;
        }

        .text-error {
            color: #ff0000 !important;
            position: inherit !important;
            padding-bottom: 15px;
        }

        .text-sucsess {
            position: inherit !important;
            color: #00B900;
            padding-bottom: 15px;
        }

        .img-rectangle {
            height: 223px !important;
        }

        @@media(max-width: 767px) {
            .download-app-img {
                display: inherit;
            }

            .whytravelwith-image img {
                max-width: 62px !important;
            }

            .sub-scribe-deal .form-email {
                padding: inherit;
            }

